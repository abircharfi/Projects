@model UserLogin 

<form asp-action="LoginUserForm" method="post" class="form-horizontal">
    

    <div class="form-group mb-3">
                <label asp-for="LoginEmail"></label>
                <span asp-validation-for="LoginEmail" class="text-danger"></span>
                <input asp-for="LoginEmail" class="form-control" value="abir@gmail.com">
     </div>
    <div class="form-group mb-3">
                <label asp-for="LoginPassword"></label>
                <span asp-validation-for="LoginPassword" class="text-danger"></span>
                <input asp-for="LoginPassword" class="form-control" value="12345678">
     </div>
    <span asp-validation-for="LoginEmail" class="text-danger"></span>
     <span asp-validation-for="LoginPassword" class="text-danger"></span>
    <div class="form-group">
         <input type="submit" value="Login" class="btn btn-primary">
    </div>
</form>